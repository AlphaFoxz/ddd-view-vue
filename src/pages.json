{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/login",
      "style": {
        "rightWindow": false,
        "leftWindow": false,
        "topWindow": false
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationBarBackgroundColor": "rgba(0,0,0,0)"
      }
    },
    {
      "path": "pages/index/3dtest"
    }
  ],
  "globalStyle": {
    "rpxCalcMaxDeviceWidth": 960, // rpx 计算所支持的最大设备宽度，单位 px，默认值为 960
    "rpxCalcBaseDeviceWidth": 375, // rpx 计算使用的基准设备宽度，设备实际宽度超出 rpx 计算所支持的最大设备宽度时将按基准宽度计算，单位 px，默认值为 375
    "rpxCalcIncludeWidth": 750, // rpx 计算特殊处理的值，始终按实际的设备宽度计算，单位 rpx，默认值为 750
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "ddd",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "rightWindow": true,
    "leftWindow": true,
    "topWindow": true
  },
  "topWindow": {
    "path": "components/layout/top-window.vue",
    "style": {
      "maxWidth": "100vw",
      "display": "flex",
      "align-items": "center",
      "justify-content": "center"
    },
    "matchMedia": {
      "minWidth": "960px"
    }
  },
  "rightWindow": {
    "path": "components/layout/right-window.vue",
    "style": {
      "width": "60vw"
    },
    "matchMedia": {
      "minWidth": "960px"
    }
  },
  "leftWindow": {
    "path": "components/layout/left-window.vue",
    "style": {
      "width": "40vw"
    },
    "matchMedia": {
      "maxWidth": "960px"
    }
  }
}
